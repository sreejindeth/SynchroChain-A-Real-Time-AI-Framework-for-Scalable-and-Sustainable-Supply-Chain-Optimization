# SynchroChain .gitignore

# Python
__pycache__/
*.py[cod]
*$py.class
*.so
.Python
*.egg
*.egg-info/
dist/
build/
*.whl

# Virtual Environment (IMPORTANT: Don't commit this!)
STRM/
env/
venv/
ENV/
env.bak/
venv.bak/

# Data files (too large for git)
data/
!data/.gitkeep
!data/raw/.gitkeep
!data/processed/.gitkeep

# Trained models (too large for git)
models/*.pth
models/*.pkl
models/**/*.pth
models/**/*.pkl
models/distilbert_tokenizer/
models/bert_tokenizer/
models/saved/*.pth
models/saved/*.pkl
models/cv_results/

# Keep model directory structure
!models/.gitkeep
!models/saved/.gitkeep

# Results and outputs (too large for git)
results/
logs/
*.log
reports/viz/

# Jupyter Notebooks
.ipynb_checkpoints/
*.ipynb

# IDE
.vscode/
.idea/
*.swp
*.swo
*~

# OS
.DS_Store
Thumbs.db
desktop.ini

# Temporary files
*.tmp
*.temp
temp/
tmp/

# Testing
.pytest_cache/
.coverage
htmlcov/
.tox/

# MyPy
.mypy_cache/
.dmypy.json
dmypy.json

# Pyre
.pyre/

# Environment variables
.env
.env.local

# Keep empty directories
!logs/.gitkeep
!results/.gitkeep
!reports/viz/.gitkeep
